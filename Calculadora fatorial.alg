algoritmo "enquantoFatorial"
// Função :
// Autor :
// Data : 12/08/2025
// Seção de Declarações 
var

num , fator : inteiro
result : real

inicio
Escreval("=*= CALCULADORA FATORIAL =*=")
escreva("Digite o número a ser feito fatorial: ")
leia(num)

fator <- num

enquanto fator > 1 faca


fator <- fator - 1

num <- fator * num

fimenquanto

escreval("Resultado: ", num)


fimalgoritmo
